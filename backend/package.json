    "babel-jest": "29.7.0",
    "concurrently": "^7.6.0",
    "jest": "29.7.0",
    "json-schema-to-typescript": "^13.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.5"
  },
  "imports": {
    "#src/*": "./src/*.ts"
  },
