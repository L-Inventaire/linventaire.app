{
  "name": "application",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn -- src/index.ts",
    "dev:test": "TZ=UTC JEST=1 jest --watchAll ",
    "build": "tspc && cp -R ./assets ./dist/ && yarn sentry:sourcemaps",
    "start": "tsc && cp -R ./assets ./dist/ && node --prof ./dist/src/index.js",
    "serve": "node --inspect ./dist/src/index.js",
    "test": "tsc && rm -R dist/tests && TZ=UTC JEST=1 CREATE_SERVER=1 NODE_ENV=tests jest --runInBand --watchAll=false --forceExit --coverage ./",
    "linter": "CI=true eslint 'src/**/*.{ts,ts}'",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org linventaireapp --project linventaire-back ./dist && sentry-cli sourcemaps upload --org linventaireapp --project linventaire-back ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "testTimeout": 20000,
    "modulePathIgnorePatterns": [
      "dist"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-pdf)/)"
    ],
    "moduleNameMapper": {
      "^#src/(.*)$": "<rootDir>/src/$1",
      "@react-pdf/renderer": "<rootDir>/tests/__mocks__/@react-pdf/renderer.ts"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
